{% extends 'base.html.twig' %}

{% block title %}NutritionalNeeds index{% endblock %}

{% block body %}
    <h1>NutritionalNeeds index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>EspeceBesoinNutritionnel</th>
                <th>StatutProductionBesoinNutritionnel</th>
                <th>SexeBesoinNutritionnel</th>
                <th>PoidsMinBesoinNutritionnel</th>
                <th>PoidsMaxBesoinNutritionnel</th>
                <th>ButeProductionBesoinNutritionnel</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for nutritional_need in nutritional_needs %}
            <tr>
                <td>{{ nutritional_need.id }}</td>
                <td>{{ nutritional_need.especeBesoinNutritionnel }}</td>
                <td>{{ nutritional_need.statutProductionBesoinNutritionnel }}</td>
                <td>{{ nutritional_need.sexeBesoinNutritionnel }}</td>
                <td>{{ nutritional_need.poidsMinBesoinNutritionnel }}</td>
                <td>{{ nutritional_need.poidsMaxBesoinNutritionnel }}</td>
                <td>{{ nutritional_need.buteProductionBesoinNutritionnel }}</td>
                <td>
                    <a href="{{ path('app_nutritional_needs_show', {'id': nutritional_need.id}) }}">show</a>
                    <a href="{{ path('app_nutritional_needs_edit', {'id': nutritional_need.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="8">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_nutritional_needs_new') }}">Create new</a>
{% endblock %}
