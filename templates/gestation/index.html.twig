{% extends 'base.html.twig' %}

{% block title %}Gestation index{% endblock %}

{% block body %}
    <h1>Gestation index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Espece</th>
                <th>PreparationVêlage</th>
                <th>VêlagePrévu</th>
                <th>DateAvertissement</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for gestation in gestations %}
            <tr>
                <td>{{ gestation.id }}</td>
                <td>{{ gestation.espece }}</td>
                <td>{{ gestation.preparationVêlage ? gestation.preparationVêlage|date('Y-m-d') : '' }}</td>
                <td>{{ gestation.vêlagePrévu ? gestation.vêlagePrévu|date('Y-m-d') : '' }}</td>
                <td>{{ gestation.dateAvertissement ? gestation.dateAvertissement|date('Y-m-d') : '' }}</td>
                <td>
                    <a href="{{ path('app_gestation_show', {'id': gestation.id}) }}">show</a>
                    <a href="{{ path('app_gestation_edit', {'id': gestation.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_gestation_new') }}">Create new</a>
{% endblock %}
